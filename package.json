{
  "name": "ts-runner-and-libary",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.1.0",
  "description": "_description_",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "start": "npm run start:dev",
    "start:dev": "esno watch src/index.ts",
    "start:prod": "pnpm install && pnpm build && node dist/index.mjs",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "esno": "^4.7.0",
    "pnpm": "^9.0.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "discord.js": "^14.15.2",
    "express": "^4.19.2",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
